  <%= form_tag "authenticate" do %>
    <div class="form-group">
      <label for="phoneNumber">Phone Number</label>
      <input type="text" class="form-control" name="phoneNumber" id="phoneNumber" placeholder="Phone Number">
    </div>
    <button class="btn btn-outline-secondary"><%= open_iconic :icon, :arrow_thick_left %>Go Back</button>
    <button type="submit" class="btn btn-primary">Submit</button>
  <% end %>